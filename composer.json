{
  "name": "robinn/filecache-dashboard",
  "description": "FileCache (robinn/cache) dashboard for phpCacheAdmin.",
  "license": "MIT",
  "type": "project",
  "keywords": [
    "file",
    "cache",
    "admin",
    "dashboard",
    "gui",
    "phpcacheadmin"
  ],
  "authors": [
    {
      "name": "Róbert Kelčák",
      "email": "robo@kelcak.com",
      "homepage": "https://kelcak.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "robinn/cache": "^2.6"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "phpunit/phpunit": "^10.4",
    "robinn/phpcacheadmin": "dev-master"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "RobiNN\\FileCache\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RobiNN\\FileCache\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "phpstan": "phpstan --ansi",
    "test": "phpunit --colors=always"
  }
}
